{
  "name": "bui-plugin",
  "displayName": "bui-plugin",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neo-rooney/bui-plugin.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bui-plugin",
          "title": "BUI Plugin",
          "icon": "$(play)"
        }
      ]
    },
    "views": {
      "bui-plugin": [
        {
          "id": "bui-plugin-view",
          "name": "BUI Plugin",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "node scripts/prepackage.js && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,mjs,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,mjs,json}\"",
    "test": "vscode-test",
    "package": "mkdir -p build && vsce package --out ./build",
    "build": "npm run package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/vscode": "^1.105.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1"
  }
}
